{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Data Management Tool",
  "readme": "./docs/api-examples.md",
  "entryPoints": ["src/**/*.ts", "src/**/*.tsx"],
  "entryPointStrategy": "expand",
  "plugin": ["typedoc-plugin-markdown", "typedoc-vitepress-theme", "typedoc-plugin-zod", "typedoc-plugin-rename-defaults"],
  "docsRoot": "./docs",
  "out": "./docs/api",
  "categorizeByGroup": true,
  "hideGenerator": true,
  "cacheBust": true,
  "navigation": {
    "includeCategories": false,
    "includeGroups": false,
    "includeFolders": false
  },
  "highlightLanguages": [
    "css",
    "html",
    "javascript",
    "json",
    "jsonc",
    "json5",
    "yaml",
    "tsx",
    "typescript"
  ],
  "skipErrorChecking": false, // true for local dev
  "sidebar": {
    "autoConfiguration": true,
    "format": "vitepress",
    "pretty": true,
    "collapsed": true
  }
}
