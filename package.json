{
  "name": "data-management-tool",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun --watch src/index.ts --target bun",
    "build": "bun build src/index.ts --target bun",
    "start": "NODE_ENV=production bun src/index.ts",
    "test": "bun test",
    "db:schema:pull": "prisma db pull",
    "db:schema:push": "prisma db push",
    "db:baseline": "prisma migrate diff",
    "db:migrate:resolve": "prisma migrate resolve",
    "db:help": "bun run prisma --help"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts --target bun"
  },
  "dependencies": {
    "@adonisjs/env": "^6.1.0",
    "@elysiajs/bearer": "latest",
    "@elysiajs/cors": "latest",
    "@elysiajs/html": "latest",
    "@elysiajs/swagger": "latest",
    "@kitajs/ts-html-plugin": "^4.0.1",
    "@prisma/client": "^5.15.0",
    "@prisma/migrate": "^5.15.0",
    "elysia": "1.1.18",
    "elysia-compression": "latest",
    "prisma": "^5.15.0",
    "prisma-schema-dsl": "^2.2.0",
    "typescript": "^5.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ts-safeql/eslint-plugin": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "bun-types": "latest",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "prettier": "^3.3.1"
  },
  "module": "src/index.ts"
}
