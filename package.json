{
  "name": "data-management-tool",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun --watch src/index.ts --target bun",
    "build": "bun build src/index.ts --target bun",
    "start": "NODE_ENV=production bun src/index.ts",
    "test": "bun test",
    "db:schema:pull": "prisma db pull",
    "db:schema:push": "prisma db push",
    "db:baseline": "prisma migrate diff",
    "db:migrate:resolve": "prisma migrate resolve",
    "db:help": "bun run prisma --help",
    "predocs:dev": "typedoc",
    "predocs:build": "typedoc",
    "docs:dev": "vitepress dev docs --host",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts --target bun"
  },
  "dependencies": {
    "@adonisjs/env": "^6.1.0",
    "@elysiajs/bearer": "latest",
    "@elysiajs/cors": "latest",
    "@elysiajs/html": "latest",
    "@elysiajs/swagger": "latest",
    "@kitajs/ts-html-plugin": "^4.0.1",
    "@prisma/client": "^5.15.0",
    "@prisma/migrate": "^5.15.0",
    "elysia": "1.0.23",
    "elysia-compression": "latest",
    "prisma": "^5.15.0",
    "prisma-schema-dsl": "^2.2.0",
    "typescript": "^5.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@stylistic/eslint-plugin-jsx": "^2.11.0",
    "@stylistic/eslint-plugin-ts": "^2.11.0",
    "@ts-safeql/eslint-plugin": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "bun-types": "latest",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.3.1",
    "typedoc": "^0.26.11",
    "typedoc-plugin-markdown": "^4.2.10",
    "typedoc-vitepress-theme": "^1.0.2",
    "typescript-eslint": "^8.15.0",
    "vitepress": "^1.3.4"
  },
  "module": "src/index.ts",
  "keywords": [
    "Data Management Tool",
    "Typescript",
    "Elysia",
    "Bun",
    "Prisma",
    "Nobey Net"
  ]
}
