{
  "name": "elysia-api",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun --watch src/index.ts --target bun",
    "build": "bun build src/index.ts --target bun",
    "start": "NODE_ENV=production bun src/index.ts",
    "test": "bun test",
    "db:schema:pull": "prisma db pull",
    "db:baseline": "prisma migrate diff",
    "db:migrate:resolve": "prisma migrate resolve",
    "db:help": "bun run prisma --help"
  },
  "dependencies": {
    "@adonisjs/env": "^5.0.1",
    "@elysiajs/bearer": "latest",
    "@elysiajs/cors": "latest",
    "@elysiajs/html": "latest",
    "@elysiajs/swagger": "latest",
    "@kitajs/ts-html-plugin": "^1.3.3",
    "@prisma/client": "^5.7.1",
    "@prisma/migrate": "^5.7.1",
    "elysia": "latest",
    "elysia-compression": "latest",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ts-safeql/eslint-plugin": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.2",
    "bun-types": "latest",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.1.1"
  },
  "module": "src/index.ts"
}
